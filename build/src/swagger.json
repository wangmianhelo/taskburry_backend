    "host": "localhost:7001",
    "basePath": "/",
    "tags": [
        {
            "name": "task",
            "description": "Everything about your Pets"
        },
        {
            "name": "user",
            "description": "Access to user info"
        },
        {
            "name": "review",
            "description": "Operations about review"
        }
